      <select id="postxxselect" resultMap="ccc"> 
  SELECT po.*,ss.`Uimg` AS uimg, ss.`Username` AS uname, ss.Email AS email FROM post AS po
INNER JOIN `customerss` AS ss
ON ss.`Id`=po.`uid` 
WHERE po.id=#{id}
  </select>
    
    
   <select id="postselect" resultMap="postdy">
  SELECT po.*,dy.url FROM post AS po
INNER JOIN dynamicpicture AS dy
ON po.id=dy.dyid
<where>
  <if test="tid!=null">
    po.`tid`=#{tid}
    </if>
</where>
  </select>
  
  
